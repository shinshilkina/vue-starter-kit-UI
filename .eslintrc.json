{
  "extends": [
    "plugin:vue/essential"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-var": "error",
    "semi": "error",
    "indent": ["error", 4],
    "no-multi-spaces": "error",
    "space-in-parens": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error",
    "no-use-before-define": "error",
    "quotes": ["error", "single"]
  }
}
